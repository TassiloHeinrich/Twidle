<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_bubble_wrap"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F497A0"
 >
    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitCenter"
        android:src="@drawable/bubble_wrap_end"
        android:onClick="onClickChangeToNotPopped"
        android:background="#F497A0"
        />

    <ImageButton
        android:id="@+id/notPoppedBubbleWrap"
        android:src="@drawable/bubble_wrap_start"
        android:scaleType="fitCenter"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F497A0"
        android:onClick="onClickChangeToPopped"
        />


</RelativeLayout>
